<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>Express Skills</title>
</head>
<body>
	<h1>My Skills</h1>
    <!-- link to get to skills/new file -->
    <a href="/skills/new">Add Skill</a>
    <!-- see data list of skills -->
    <ul>
      <% skills.forEach(function(s) { %>
        <li>
          
          <a href="/skills/<%= s.id %>"> <%= s.skill %> </a>
            - 
          <%= s.understand ? 'understand' : 'kinda understand' %>
          
        </li>
      <% }); %>
    </ul>
	
</body>
</html>